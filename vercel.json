{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/docs/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/v1/products/(.*)",
        "methods": ["GET", "POST"],
        "dest": "src/server.js"
      },
      {
        "src": "/api/v1/products/delete/:slug",
        "methods": ["DELETE"],
        "dest": "src/server.js"
      },
      {
        "src": "/docs",
        "dest": "src/docs/index.html"
      },
      {
        "src": "/",
        "status": 302,
        "headers": {
          "Location": "/docs"
        }
      }
    ]
  }
  